library(tidycovid19)
library(dplyr)
library(foreach)
all_countries <- download_merged_data(cached = TRUE)
calculate_daily_diffs <- function(country_df){
country_df$deaths_daily <- c(diff(country_df$deaths, 1), NA)
country_df$confirmed_daily <- c(diff(country_df$confirmed, 1), NA)
country_df$recovered_daily <- c(diff(country_df$recovered, 1), NA)
country_df
}
all_countries <- foreach(country = unique(all_countries$iso3c),
.combine = 'rbind') %do% {
country_df = all_countries[all_countries$iso3c == country,]
calculate_daily_diffs(country_df)
}
spain <- all_countries %>% filter(iso3c == "ESP")
other_countries <-  all_countries %>% filter(iso3c != "ESP")
